package com.common;

import java.io.Serializable;

/**
 * @author King
 *
 */
public class StatusFormatBean implements Serializable{
	private int statusKey;
	private String statusString;

	/**
	 * 
	 */
	public StatusFormatBean() {
		super();
	}

	/**
	 * @param statusKey
	 * @param statusString
	 */
	public StatusFormatBean(int statusKey, String statusString) {
		super();
		this.statusKey = statusKey;
		this.statusString = statusString;
	}

	/**
	 * @return the statusKey
	 */
	public int getStatusKey() {
		return statusKey;
	}

	/**
	 * @param statusKey
	 *            the statusKey to set
	 */
	public void setStatusKey(int statusKey) {
		this.statusKey = statusKey;
	}

	/**
	 * @return the statusString
	 */
	public String getStatusString() {
		return statusString;
	}

	/**
	 * @param statusString
	 *            the statusString to set
	 */
	public void setStatusString(String statusString) {
		this.statusString = statusString;
	}

}
